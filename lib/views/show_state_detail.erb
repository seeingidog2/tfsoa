<title>tfsoa <%= @state_detail.tfstate.unique_tf_state %> #<%= @state_detail.id %></title>

<h2><%= @state_detail.tfstate.unique_tf_state %> #<%= @state_detail.id %></h2>

<% if @state_detail.digraph %>
  <a href=<%= "/render_graph/#{@state_detail.id}"%>>Graph</a>
<% end %>

<ul>
  <li><b>team</b>: <%= @state_detail.tfstate.team %></li>
  <% @state_detail.attributes.each do |detail| %>
    <li><b><%= detail[0] %></b>: <%= detail[1] %></li>
  <% end %>
</ul>
